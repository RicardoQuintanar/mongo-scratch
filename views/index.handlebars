<div class="container">
    {{#each article}}
    {{#unless isSaved}}
    <div>

        <a href={{this.url}}>
            <h3>{{this.headline}}</h3>
        </a>
        <p>{{this.summary}}</p>
    </div>
    {{/unless}}
    {{/each}}

    {{#if empty}}
    <div>
        <p class="text-center">
            No info, please scrape and render some here.

        </p>
    </div>
    {{/if}}
</div>